@PART[KK_SpXCD_mainChutes]:FINAL
{
	@MODULE[ModuleParachute]
	{
		@deployAltitude = 2000
		@stowedDrag = 0.25
		@fullyDeployedDrag = 600
		@minAirPressureToOpen = 0.02
		@clampMinAirPressure = 0.02
	}
	@MODULE[ModuleDragModifier]:HAS[#dragCubeName[DEPLOYED]]
	{
		dragModifier = 200
	}
}


@PART[KK_SpXCD_drogueChutes]:FINAL
{
	@MODULE[ModuleParachute]
	{
		@deployAltitude = 5800
		@stowedDrag = 0.25
		@fullyDeployedDrag = 100
		@minAirPressureToOpen = 0.01
		@clampMinAirPressure = 0.01
	}
}